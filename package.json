	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"format": "biome format src --write",
		"lint": "biome lint --apply --files-ignore-unknown=true --colors=force --log-level=info src",
		"test": "npx playwright test",
		"test:page": "npx playwright test --grep @page",
		"test:a11y": "npx playwright test --grep @a11y",
		"test:report": "yarn playwright show-report"
	},
